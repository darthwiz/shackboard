<% bw = 1 %>
<% ts = 6 %>
<div id="navbar_top">
  <%# render :partial => '/layouts/navbar' %>
</div>
<div id="pms">
  <ul id="pmlist">
    <% @pms.each do |pm| %>
    <li>
      <ul>
        <li class="userinfo">
          <%= pm.from.username %>
        </li>
        <li class="pm">
          <%= bb_to_html(pm.message) %>
        </li>
      </ul>
      <div/>
    </li>
    <% end %>
  </ul>
  <div id="navbar_bottom">
    <%# render :partial => '/layouts/navbar' %>
  </div>
</div>
