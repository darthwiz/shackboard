<br/>
<table id="cms_pages_index" class="themed" width="100%" cellspacing="1">
  <tr>
    <th>id</th>
    <th>titolo</th>
    <th>slug</th>
    <th>autore</th>
    <th>data</th>
  </tr>
  <%@cms_pages.each do |p|%>
  <tr>
    <td><%=p.id%></td>
    <td><%=cleanup(p.title)%></td>
    <td><%=p.slug%></td>
    <td><%=cleanup(p.user.username)%></td>
    <td><%=p.created_at%></td>
  </tr>
  <%end%>
</table>
<div class="new_cms_page_link">
  <%=link_to 'nuova pagina', new_admin_cms_page_path%>
</div>
